{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 8,
    "requireConfigFile": false,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true
    }
  },
  "rules": {
    "import": 0,
    "import/extensions": 0,
    "max-len": 1,
    "no-console": 0,
    "comma-dangle": 0,
    "import/no-cycle": 1,
    "linebreak-style": 1,
    "no-return-assign": 1,
    "consistent-return": 1,
    "no-param-reassign": 1,
    "no-use-before-define": 0,
    "no-underscore-dangle": 1,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "no-unused-vars": [
      1,
      {
        "ignoreRestSiblings": false
      }
    ],
    "prettier/prettier": [
      2,
      {
        "printWidth": 100,
        "singleQuote": true,
        "trailingComma": "none",
        "tabWidth": 2
      }
    ]
  }
}
